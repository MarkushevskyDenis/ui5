<mvc:View
    controllerName="ui5demo.controller.Second"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
    displayBlock="true"
	xmlns="sap.m"
	xmlns:c="ui5demo.control">

	<Page>

		<c:ProductRate valueSubmit = "onSubmit" />

		<Panel id = "productsDetailPanel">
			<l:Grid>
				<Text text = "date : {Erdate}"  />
			</l:Grid>
		</Panel>

		<List id = "ListId" headerText="{i18n>title}" width = "50%" mode = "SingleSelectMaster" selectionChange = "onSelectionChange" items="{/results}">
			<items>
				<ObjectListItem title="{Currencykey}" number="{path:'Erdate', formatter:'.formatter.dateFormat'}" intro="{Qty}"/>
			</items>
		</List>

	</Page>

</mvc:View>
